{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/transform-runtime",
        "@babel/plugin-proposal-class-properties",
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        "@babel/transform-runtime",
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
